<% 
  let currentLangText = '';
  let currentLangFlagName = '';
  switch (lang) {
    case 'en':
      currentLangText = 'EN';
      currentLangFlagName = 'en-lang-flag.png';
      break;
    case 'id':
      currentLangText = 'ID';
      currentLangFlagName = 'id-lang-flag.png';
      break;
    case 'kr':
      currentLangText = 'KO';
      currentLangFlagName = 'ko-lang-flag.png';
      break;
  
    default:
      currentLangText = 'EN';
      currentLangFlagName = 'en-lang-flag.png';
      break;
  }
%>

<%- include('partials/header.ejs') %>
  <body class="dream-concert-event <%= lang %>">
      <section class="ticket__metaverse ">
        <div class="container metaverse_wrap">
          <div class="row">
            <div class="col">
              <img src="/img/digifinex.svg" alt="" class="image-banner">
            </div>
          </div>
          <div class="row ">
            <!-- <div class="backround__white">
            </div> -->
            <div class="col-md-12 ticket_card ">
              <div class="card">
                <div class="card-body">
                  <h5 class="card-title">29TH DREAM CONCERT IN BUSAN</h5>
                  <p class="card-text">METAVERSE TICKET.</p>
                  <p class="card-text">May 27 th, 2023.</p>
                  <div class="row">
                    <div class="col-xl-12">
                      <div class="text__small">
                        <p class="card_small_text">Purchasing BSI at</p>
                        <img src="/img/digifinex.svg" alt="" class="text-image">
