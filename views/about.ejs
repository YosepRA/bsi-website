<% 
  const { about: {
    groupCompany,
    bsiTeam,
    partnership
  }, navbar: {
    about: {
      subcategory: [
        groupCompanyMeta,
        bsiTeamMeta,
        partnershipMeta,
      ]
    }
  } } = data;
%>

<%- include('partials/header.ejs') %>
  <body class="about <%= lang %>">
    <%- include('partials/navbar.ejs') %>

    <main class="main-container">
      <section id="<%= groupCompanyMeta.id %>" class="group-company page-section container-fluid">
        <div class="row group-company__text-row">
          <div class="col-12 group-company__col">
            <h2 class="group-company__title page-section__title"><%= groupCompany.title %></h2>
          </div>
        </div>

        <div class="row group-company__image-row">
          <div class="col-12 group-company__col">
            <img src="/img/group-company-white.png" alt="Viaje Foundation" class="group-company__image" />
          </div>
        </div>
      </section>

      <section id="<%= bsiTeamMeta.id %>" class="bsi-team page-section container">
        <div class="row bsi-team__row bsi-team__row--title">
          <div class="col">
            <h2 class="bsi-team__title page-section__title"><%= bsiTeam.title %></h2>
          </div>
        </div>

        <div class="row bsi-team__row bsi-team__row--images">
          <div class="col-12 col-md-5 col-lg-4 col-xl-4">
            <div class="bsi-team__item bsi-team__item--danniel">
              <div class="bsi-team__item-overlay"></div>

              <div class="bsi-team__item-info">
                <h4 class="bsi-team__item-name">Danniel Prakasa</h4>
                <h5 class="bsi-team__item-position">CEO / Chief Executive Officer</h5>
              </div>
            </div>
          </div>

          <div class="col-12 col-md-5 col-lg-4 col-xl-4">
            <div class="bsi-team__item bsi-team__item--juleo">
              <div class="bsi-team__item-overlay"></div>

              <div class="bsi-team__item-info">
                <h4 class="bsi-team__item-name">Juleo Fernando</h4>
                <h5 class="bsi-team__item-position">COO / Chief Operations Officer</h5>
              </div>
            </div>
          </div>
        </div>
      </section>

      <section id="<%= partnershipMeta.id %>" class="partnership page-section container">
        <div class="row">
          <div class="col-12">
            <h2 class="partnership__title page-section__title"><%= partnership.title %></h2>
          </div>

          <div class="col-12">
            <% for( let i = 0; i < partnership.body.length; i++ ) { %>
              <% const text = partnership.body[i]; %>

              <p class="partnership__body"><%- text %></p>  
            <% } %>

            <!-- <p class="partnership__body">Lorem ipsum dolor sit amet consectetur, adipisicing elit. Nemo accusantium tempore inventore eveniet possimus? Perspiciatis ut veritatis id porro aliquid?</p> -->
          </div>
        </div>

        <div class="row partnership__images-row">
          <div class="col-12">
            <div class="row partnership__level-row partnership__level-one">
              <div class="col-3 partnership__level-col">
                <img src="/img/MBI-logo.png" alt="MBI" class="partnership__image partnership__image--mbi" />
              </div>
              <div class="col-3 partnership__level-col">
                <img src="/img/natya-hotel-logo-white.png" alt="Natya Hotel and Resort" class="partnership__image partnership__image--natya" />
              </div>
              <div class="col-3 partnership__level-col">
                <img src="/img/coco-mart-logo-white.png" alt="Coco Supermarket" class="partnership__image partnership__image--coco" />
              </div>
            </div>

            <div class="row partnership__level-row partnership__level-two">
              <div class="col-3 partnership__level-col">
                <img src="/img/natys-restaurant-logo-white.png" alt="Natys Restaurant" class="partnership__image partnership__image--natys" />
              </div>
              <div class="col-3 partnership__level-col">
                <img src="/img/smoot-logo-white.png" alt="Smoot" class="partnership__image partnership__image--smoot" />
              </div>
              <div class="col-3 partnership__level-col">
                <img src="/img/swap-logo-white.png" alt="Swap" class="partnership__image partnership__image--swap" />
              </div>
              <div class="col-3 partnership__level-col">
                <img src="/img/volta-logo-white.png" alt="Volta" class="partnership__image partnership__image--volta" />
              </div>
            </div>

            <div class="row partnership__level-row partnership__level-three">
              <div class="col-12 partnership__level-col">
                <img src="/img/partners-level-3.png" alt="Partners Level 3" class="partnership__image partnership__image--level-3" />
              </div>
            </div>
          </div>
        </div>
      </section>
    </main>

    <%- include('partials/floating-buttons.ejs') %>
    <%- include('partials/footer.ejs') %>
    <%- include('partials/js-libraries.ejs') %>
  </body>
</html>
