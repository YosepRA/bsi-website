<% 
  const { service: {
    viajeEcosystem,
    viajePlatform,
    metaConcert,
  }, navbar: {
    service: {
      subcategory: [
        viajeEcosystemMeta,
        viajePlatformMeta,
        metaConcertMeta
      ]
    }
  } } = data;
%>

<%- include('partials/header.ejs') %>

  <body class="service <%= lang %>">
    <%- include('partials/navbar.ejs') %>

    <main class="main-container">
      <section id="<%= viajeEcosystemMeta.id %>" class="viaje_ecosystem">
        <div class="container">
          <div class="row align-items-center">
            <div class="col">
              <h4 class="big-title text-center">
                <%= viajeEcosystem.title %>
              </h4>
  
              <% for( let i = 0; i < viajeEcosystem.body.length; i++ ) { %>
                <% const text = viajeEcosystem.body[i]; %>
  
                <p class="text-white"><%- text %></p>
              <% } %>
            </div>
          </div>
          <div class="container">
            <div class="row text-center viaje_ecosystem_picture">
              <div class="col-lg">
                <img src="/img/viaje ecosystem.png" alt="viaje-ecosystem" class="viaje_ecosystem_icon"/>
              </div>
            </div>
          </div>
        </div>
      </section>
  
      <section id="<%= viajePlatformMeta.id %>" class="viaje_platform">
        <div class="container">
          <div class="row align-items-center">
            <div class="col">
              <h4 class="big-title text-center">
                <%= viajePlatform.title %>
              </h4>
  
              <% for( let i = 0; i < viajePlatform.body.length; i++ ) { %>
                <% const text = viajePlatform.body[i]; %>
  
                <p class="text-white"><%- text %></p>
              <% } %>
            </div>
          </div>
  
          <div class="row text-center viaje_platform_page">
            <div class="col-md-12">
              <img src="/img/viaje platform.png" alt="viaje_platform" class="viaje_platform_picture"/>
            </div>
          </div>
        </div>
      </section>
  
      
        <section id="<%= metaConcertMeta.id %>" class="meta_concert_illust">
          <div class="container">
            <div class="row meta_concert_prop">
              <div class="col-md-6 meta_img">
                <img class="meta-concert-img" src="/img/meta concert illust.png" alt="meta-concert-illust">
              </div>
              <div class="col">
                <h4 class="big-title"><%= metaConcert.title %></h4>
        
                <% for( let i = 0; i < metaConcert.body.length; i++ ) { %>
                  <% const text = metaConcert.body[i]; %>
        
                  <p class="small-1 text-white"><%- text %></p>
                <% } %>    
              </div>
            </div>
          </div>
      </section>
    </main>

    <%- include('partials/footer.ejs') %>
    <%- include('partials/js-libraries.ejs') %>
  </body>
</html>
